{
  "name": "oas-raml-converter",
  "version": "0.1.11",
  "description": "This package helps to convert between different API specifications. Supports OAS (Swagger 2), RAML 0.8, RAML 1.0.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register",
    "lint": "gulp lint",
    "coverall": "gulp coverall"
  },
  "bin": {
    "oas-raml-converter": "./lib/bin/converter.js",
    "raml-parser": "./lib/bin/raml-parser.js",
    "oas-parser": "./lib/bin/oas-parser.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mulesfot/api-spec-converter.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mulesoft/api-spec-converter/issues"
  },
  "homepage": "https://mulesoft.github.io/api-spec-converter",
  "dependencies": {
    "js-yaml": "3.x.x",
    "json-schema-compatibility": "1.x.x",
    "jsonschema": "1.x.x",
    "lodash": "4.x.x",
    "raml-1-parser": "1.1.x",
    "raml-xml-validation": "0.0.x",
    "request": "2.x.x",
    "swagger-parser": "4.0.0-beta.2"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.14.0",
    "browser-request": "^0.3.3",
    "browserify-global-shim": "^1.0.3",
    "chai": "^3.5.0",
    "chai-fuzzy": "1.x.x",
    "chai-string": "^1.2.0",
    "commander": "^2.9.0",
    "coveralls": "^2.11.8",
    "gulp": "^3.9.1",
    "gulp-coveralls": "^0.1.4",
    "gulp-eslint": "^2.0.0",
    "jscover": "^1.0.0",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0"
  },
  "browser": {
    "request": "browser-request"
  },
  "browserify": {
    "transform": [
      "browserify-global-shim"
    ]
  },
  "browserify-global-shim": {
    "raml-1-parser": "RAML.Parser"
  },
  "directories": {
    "example": "example",
    "test": "test"
  }
}
