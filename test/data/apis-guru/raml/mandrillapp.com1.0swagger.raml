#%RAML 1.0
title: Mandrill
version: '1.0'
baseUri: 'https://mandrillapp.com/api/'
protocols:
  - HTTPS
description: |
  Mandrill is a reliable, scalable, and secure delivery API for transactional emails from websites and applications. It's ideal for sending data-driven transactional emails, including targeted e-commerce and personalized one-to-one messages.
(oas-info):
  (oas-x-logo):
    url: 'https://api.apis.guru/v2/cache/logo/http_mandrill.com_assets_images_brand_logo-vertical-light-735b5657.png'
  (oas-x-origin):
    format: swagger
    url: 'https://raw.githubusercontent.com/APIs-guru/unofficial_openapi_specs/master/mandrillapp.com/1.0/swagger.yaml'
    version: '2.0'
  (oas-x-preferred): true
  (oas-x-providerName): mandrillapp.com
  (oas-x-unofficialSpec): true
annotationTypes:
  oas-x-logo: any
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-x-unofficialSpec: any
  oas-externalDocs:
    properties:
      description?: string
      url: string
    allowedTargets:
      - API
      - Method
      - TypeDeclaration
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
  oas-body-name:
    type: string
    allowedTargets: TypeDeclaration
(oas-externalDocs):
  url: 'https://mandrillapp.com/api/docs/'
/senders:
  displayName: senders
  /add-domain.json:
    displayName: add-domain.json
    post:
      displayName: POST_senders-add-domain-json
      description: 'Adds a sender domain to your account. Sender domains are added automatically as you send, but you can use this call to add them ahead of time.'
      body:
        application/json:
          type: domain
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: sender_domain_info
          description: OK
  /check-domain.json:
    displayName: check-domain.json
    post:
      displayName: POST_senders-check-domain-json
      description: 'Checks the SPF and DKIM settings for a domain. If you haven''t already added this domain to your account, it will be added automatically.'
      body:
        application/json:
          type: domain
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: sender_domain_info
          description: OK
  /domains.json:
    displayName: domains.json
    post:
      displayName: POST_senders-domains-json
      description: Returns the sender domains that have been added to this account.
      body:
        application/json:
          type: api_key
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: senders_domains-response
          description: OK
  /info.json:
    displayName: info.json
    post:
      displayName: POST_senders-info-json
      description: 'Return more detailed information about a single sender, including aggregates of recent stats'
      body:
        application/json:
          type: sender_address
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: senders_info-response
          description: OK
  /list.json:
    displayName: list.json
    post:
      displayName: POST_senders-list-json
      description: Return the senders that have tried to use this account.
      body:
        application/json:
          type: api_key
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: senders
          description: OK
  /time-series.json:
    displayName: time-series.json
    post:
      displayName: POST_senders-time-series-json
      description: Return the recent history (hourly stats for the last 30 days) for a sender
      body:
        application/json:
          type: sender_address
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: time-series
          description: OK
  /verify-domain.json:
    displayName: verify-domain.json
    post:
      displayName: POST_senders-verify-domain-json
      description: 'Sends a verification email in order to verify ownership of a domain. Domain verification is an optional step to confirm ownership of a domain. Once a domain has been verified in a Mandrill account, other accounts may not have their messages signed by that domain unless they also verify the domain. This prevents other Mandrill accounts from sending mail signed by your domain.'
      body:
        application/json:
          type: senders_verify-domain
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: senders_verify-domain-response
          description: OK
/whitelists:
  displayName: whitelists
  /list.json:
    displayName: list.json
    post:
      displayName: POST_whitelists-list-json
      description: Retrieves your email rejection whitelist. You can provide an email address or search prefix to limit the results. Returns up to 1000 results.
      body:
        application/json:
          type: email
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: whitelists_list-response
          description: OK
  /delete.json:
    displayName: delete.json
    post:
      displayName: POST_whitelists-delete-json
      description: Removes an email address from the whitelist.
      body:
        application/json:
          type: email
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: whitelists_delete-response
          description: OK
  /add.json:
    displayName: add.json
    post:
      displayName: POST_whitelists-add-json
      description: 'Adds an email to your email rejection whitelist. If the address is currently on your blacklist, that blacklist entry will be removed automatically.'
      body:
        application/json:
          type: email
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: whitelists_add-response
          description: OK
/exports:
  displayName: exports
  /activity.json:
    displayName: activity.json
    post:
      displayName: POST_exports-activity-json
      description: 'Begins an export of your activity history. The activity will be exported to a zip archive containing a single file named activity.csv in the same format as you would be able to export from your account''s activity view. It includes the following fields: Date, Email Address, Sender, Subject, Status, Tags, Opens, Clicks, Bounce Detail. If you have configured any custom metadata fields, they will be included in the exported data.'
      body:
        application/json:
          type: exports_activity
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: exports_satus
          description: OK
  /list.json:
    displayName: list.json
    post:
      displayName: POST_exports-list-json
      description: Returns a list of your exports.
      body:
        application/json:
          type: api_key
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: exports_list-response
          description: OK
  /whitelist.json:
    displayName: whitelist.json
    post:
      displayName: POST_exports-whitelist-json
      description: 'Begins an export of your rejection whitelist. The whitelist will be exported to a zip archive containing a single file named whitelist.csv that includes the following fields: email, detail, created_at.'
      body:
        application/json:
          type: notify_email
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: exports_satus
          description: OK
  /rejects.json:
    displayName: rejects.json
    post:
      displayName: POST_exports-rejects-json
      description: 'Begins an export of your rejection blacklist. The blacklist will be exported to a zip archive containing a single file named rejects.csv that includes the following fields: email, reason, detail, created_at, expires_at, last_event_at, expires_at.'
      body:
        application/json:
          type: notify_email
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: exports_satus
          description: OK
  /info.json:
    displayName: info.json
    post:
      displayName: POST_exports-info-json
      description: 'Returns information about an export job. If the export job''s state is ''complete'', the returned data will include a URL you can use to fetch the results. Every export job produces a zip archive, but the format of the archive is distinct for each job type. The api calls that initiate exports include more details about the output format for that job type.'
      body:
        application/json:
          type: id
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: exports_info-response
          description: OK
/inbound:
  displayName: inbound
  /add-route.json:
    displayName: add-route.json
    post:
      displayName: POST_inbound-add-route-json
      description: Add a new mailbox route to an inbound domain
      body:
        application/json:
          type: inbound_add-route
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: route
          description: OK
  /delete-domain.json:
    displayName: delete-domain.json
    post:
      displayName: POST_inbound-delete-domain-json
      description: Delete an inbound domain from the account. All mail will stop routing for this domain immediately.
      body:
        application/json:
          type: domain
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: inbound_info
          description: OK
  /domains.json:
    displayName: domains.json
    post:
      displayName: POST_inbound-domains-json
      description: List the domains that have been configured for inbound delivery
      body:
        application/json:
          type: api_key
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: inbound_domains-response
          description: OK
  /send-raw.json:
    displayName: send-raw.json
    post:
      displayName: POST_inbound-send-raw-json
      description: 'Take a raw MIME document destined for a domain with inbound domains set up, and send it to the inbound hook exactly as if it had been sent over SMTP'
      body:
        application/json:
          type: inbound_send-raw
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: inbound_send-raw-response
          description: OK
  /check-domain.json:
    displayName: check-domain.json
    post:
      displayName: POST_inbound-check-domain-json
      description: Check the MX settings for an inbound domain. The domain must have already been added with the add-domain call
      body:
        application/json:
          type: domain
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: inbound_info
          description: OK
  /routes.json:
    displayName: routes.json
    post:
      displayName: POST_inbound-routes-json
      description: List the mailbox routes defined for an inbound domain
      body:
        application/json:
          type: domain
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: inbound_routes-response
          description: OK
  /add-domain.json:
    displayName: add-domain.json
    post:
      displayName: POST_inbound-add-domain-json
      description: Add an inbound domain to your account
      body:
        application/json:
          type: domain
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: inbound_info
          description: OK
  /update-route.json:
    displayName: update-route.json
    post:
      displayName: POST_inbound-update-route-json
      description: 'Update the pattern or webhook of an existing inbound mailbox route. If null is provided for any fields, the values will remain unchanged.'
      body:
        application/json:
          type: inbound_update-route
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: route
          description: OK
  /delete-route.json:
    displayName: delete-route.json
    post:
      displayName: POST_inbound-delete-route-json
      description: Delete an existing inbound mailbox route
      body:
        application/json:
          type: id
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: route
          description: OK
/subaccounts:
  displayName: subaccounts
  /add.json:
    displayName: add.json
    post:
      displayName: POST_subaccounts-add-json
      description: Add a new subaccount
      body:
        application/json:
          type: subaccount_info
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: subaccount_info2
          description: OK
  /delete.json:
    displayName: delete.json
    post:
      displayName: POST_subaccounts-delete-json
      description: 'Delete an existing subaccount. Any email related to the subaccount will be saved, but stats will be removed and any future sending calls to this subaccount will fail.'
      body:
        application/json:
          type: id
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: subaccount_info2
          description: OK
  /info.json:
    displayName: info.json
    post:
      displayName: POST_subaccounts-info-json
      description: 'Given the ID of an existing subaccount, return the data about it'
      body:
        application/json:
          type: id
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: subaccounts_info-response
          description: OK
  /list.json:
    displayName: list.json
    post:
      displayName: POST_subaccounts-list-json
      description: 'Get the list of subaccounts defined for the account, optionally filtered by a prefix'
      body:
        application/json:
          type: url_key
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: subaccounts_list-response
          description: OK
  /pause.json:
    displayName: pause.json
    post:
      displayName: POST_subaccounts-pause-json
      description: Pause a subaccount's sending. Any future emails delivered to this subaccount will be queued for a maximum of 3 days until the subaccount is resumed.
      body:
        application/json:
          type: id
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: subaccount_info2
          description: OK
  /resume.json:
    displayName: resume.json
    post:
      displayName: POST_subaccounts-resume-json
      description: Resume a paused subaccount's sending
      body:
        application/json:
          type: id
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: subaccount_info2
          description: OK
  /update.json:
    displayName: update.json
    post:
      displayName: POST_subaccounts-update-json
      description: Update an existing subaccount
      body:
        application/json:
          type: subaccount_info
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: subaccount_info2
          description: OK
