            application/json:
              type: ips_list-pools-response
          description: OK
  /pool-info.json:
    displayName: pool-info.json
    post:
      displayName: POST_ips-pool-info-json
      description: Describes a single dedicated IP pool.
      body:
        application/json:
          type: ips_pool-key
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: ips_pool
          description: OK
  /set-custom-dns.json:
    displayName: set-custom-dns.json
    post:
      displayName: POST_ips-set-custom-dns-json
      description: Configures the custom DNS name for a dedicated IP.
      body:
        application/json:
          type: ip_domain
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: ip_info
          description: OK
  /start-warmup.json:
    displayName: start-warmup.json
    post:
      displayName: POST_ips-start-warmup-json
      description: 'Begins the warmup process for a dedicated IP. During the warmup process, Mandrill will gradually increase the percentage of your mail that is sent over the warming-up IP, over a period of roughly 30 days. The rest of your mail will be sent over shared IPs or other dedicated IPs in the same pool.'
      body:
        application/json:
          type: ip
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: ip_info
          description: OK
  /check-custom-dns.json:
    displayName: check-custom-dns.json
    post:
      displayName: POST_ips-check-custom-dns-json
      description: Tests whether a domain name is valid for use as the custom reverse DNS for a dedicated IP.
      body:
        application/json:
          type: ip_domain
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: ips_check-custom-dns-response
          description: OK
  /info.json:
    displayName: info.json
    post:
      displayName: POST_ips-info-json
      description: Retrieves information about a single dedicated ip.
      body:
        application/json:
          type: ip
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: ip_info
          description: OK
  /provision.json:
    displayName: provision.json
    post:
      displayName: POST_ips-provision-json
      description: 'Requests an additional dedicated IP for your account. Accounts may have one outstanding request at any time, and provisioning requests are processed within 24 hours.'
      body:
        application/json:
          type: ips_provision
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: ips_provision-response
          description: OK
  /list.json:
    displayName: list.json
    post:
      displayName: POST_ips-list-json
      description: Lists your dedicated IPs.
      body:
        application/json:
          type: api_key
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: ips_list-response
          description: OK
  /set-pool.json:
    displayName: set-pool.json
    post:
      displayName: POST_ips-set-pool-json
      description: Moves a dedicated IP to a different pool.
      body:
        application/json:
          type: ips_set-pool
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: ip_info
          description: OK
  /delete-pool.json:
    displayName: delete-pool.json
    post:
      displayName: POST_ips-delete-pool-json
      description: 'Deletes a pool. A pool must be empty before you can delete it, and you cannot delete your default pool.'
      body:
        application/json:
          type: ips_pool-key
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: ips_delete-pool-response
          description: OK
