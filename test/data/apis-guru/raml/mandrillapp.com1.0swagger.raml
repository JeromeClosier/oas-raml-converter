  /create-pool.json:
    displayName: create-pool.json
    post:
      displayName: POST_ips-create-pool-json
      description: 'Creates a pool and returns it. If a pool already exists with this name, no action will be performed.'
      body:
        application/json:
          type: ips_pool-key
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: ips_pool
          description: OK
  /delete.json:
    displayName: delete.json
    post:
