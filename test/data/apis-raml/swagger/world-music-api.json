{
  "swagger": "2.0",
  "info": {
    "version": "v1",
    "title": "World Music API",
    "description": "This is an example of a music API."
  },
  "schemes": [
    "http",
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "securityDefinitions": {
    "oauth_2_0": {
      "type": "oauth2",
      "flow": "accessCode",
      "scopes": {},
      "description": "Dropbox supports OAuth 2.0 for authenticating all API requests.\n",
      "authorizationUrl": "https://www.dropbox.com/1/oauth2/authorize",
      "tokenUrl": "https://api.dropbox.com/1/oauth2/token"
    }
  },
  "paths": {
    "/api": {
      "post": {
        "operationId": "POST_api",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "$ref": "#/definitions/ApiLib.RamlDataType"
            }
          }
        ],
        "responses": {
          "default": {
            "description": "",
            "schema": {}
          }
        }
      },
      "get": {
        "operationId": "GET_api",
        "parameters": [
          {
            "name": "start",
            "in": "query",
            "required": false,
            "type": "number"
          },
          {
            "name": "page-size",
            "in": "query",
            "required": false,
            "type": "number"
          }
        ],
        "responses": {
          "default": {
            "description": "",
            "schema": {}
          }
        }
      }
    },
    "/entry": {
      "get": {
        "operationId": "GET_entry",
        "description": "returns a list of entry",
        "responses": {
          "200": {
            "description": "",
            "schema": {
              "$ref": "#/definitions/Entry"
            }
          }
        }
      },
      "post": {
        "operationId": "POST_entry",
        "responses": {
          "200": {
            "description": "",
            "schema": {
              "$ref": "#/definitions/AnotherEntry"
            }
          }
        }
      }
    },
    "/songs": {
      "post": {
        "operationId": "POST_songs",
        "parameters": [
          {
            "$ref": "#/parameters/trait:secured:access_token"
          }
        ],
        "responses": {
          "default": {
            "description": "",
            "schema": {}
          }
        }
      },
      "get": {
        "operationId": "GET_songs",
        "parameters": [
          {
            "name": "genre",
            "in": "query",
            "description": "filter the songs by genre",
            "required": true,
            "type": "string"
          },
          {
            "$ref": "#/parameters/trait:secured:access_token"
          }
        ],
        "responses": {
          "default": {
            "description": "",
            "schema": {}
          }
        },
        "security": [
          {
            "oauth_2_0": []
          }
        ],
        "x-raml-annotation-monitoringInterval": 30
      },
      "x-raml-is": [
        "secured"
      ],
      "x-raml-annotation-ready": "",
      "x-raml-resource-displayName": "Songs",
      "x-raml-resource-description": "Access to all songs inside the music world library."
    },
    "/songs/{songId}": {
      "parameters": [
        {
          "name": "songId",
          "in": "path",
          "required": true,
          "type": "string"
        }
      ],
      "get": {
        "operationId": "GET_songs-songId",
        "produces": [
          "application/xml"
        ],
        "responses": {
          "200": {
            "description": "",
            "schema": {
              "type": "object"
            },
            "examples": {
              "application/json": "<?xml version=\"1.0\" encoding=\"utf-8\"?>\n<song>\n  <title>str1234</title>\n  <artist>str1234</artist>\n</song>\n"
            }
          }
        },
        "x-raml-annotation-deprecated": {
          "date": "2016-02-28T16:41:41.090Z",
          "deprecatedBy": {
            "firstname": "Christian"
          },
          "comment": "no comment"
        }
      }
    }
  },
  "parameters": {
    "trait:secured:access_token": {
      "name": "access_token",
      "in": "query",
      "required": true,
      "type": "string"
    }
  },
  "x-basePath": "{environment}.musicapi.com/{version}",
  "definitions": {
    "Entry": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "artist": {
            "type": "string"
          }
        }
      }
    },
    "AnotherEntry": {
      "description": "This is just another entry to simulate that you can add facets also on JSON\nschema defined types. Although you can only add documentation-based facets.\n",
      "$ref": "#/definitions/Entry"
    },
    "User": {
      "example": {
        "firstname": "John"
      },
      "properties": {
        "firstname": {
          "type": "string"
        },
        "lastname": {
          "type": "string",
          "example": "Doe"
        }
      },
      "type": "object",
      "required": [
        "firstname"
      ]
    },
    "ApiLib.RamlDataType": {
      "type": "object",
      "properties": {
        "propString": {
          "type": "string"
        },
        "propStringArray1": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "ideas": {
          "type": "array",
          "items": {
            "properties": {
              "comment": {
                "type": "string"
              }
            }
          }
        },
        "extIdeas": {
          "properties": {
            "createdBy": {
              "type": "string"
            }
          },
          "type": "object",
          "required": [
            "createdBy"
          ]
        },
        "feedback": {
          "type": "string",
          "default": "very nice",
          "example": "very well made",
          "pattern": "[a-zA-Z\\s]*",
          "minLength": 1,
          "maxLength": 255
        },
        "propNumber": {
          "type": "number",
          "minimum": 0,
          "maximum": 32,
          "format": "int32",
          "multipleOf": 2
        },
        "propInteger": {
          "type": "integer",
          "minimum": 3,
          "maximum": 5,
          "format": "int8",
          "multipleOf": 1
        },
        "propBoolean": {
          "type": "boolean"
        },
        "propDate": {
          "type": "string",
          "example": "2015-05-23",
          "format": "date"
        },
        "userPicture": {
          "type": "string",
          "maxLength": 307200,
          "x-raml-type": "file",
          "x-raml-fileTypes": [
            {},
            {}
          ]
        },
        "NilValue": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "comment": {
              "type": "object"
            }
          },
          "required": [
            "name",
            "comment"
          ]
        },
        "CatOrDog": {
          "type": "object"
        },
        "CatAndDog": {
          "x-raml-union-type-definition": "[ApiLib.Cat,ApiLib.Dog]",
          "allOf": [
            {
              "$ref": "#/definitions/ApiLib.Cat"
            },
            {
              "$ref": "#/definitions/ApiLib.Dog"
            }
          ]
        },
        "PossibleMeetingDate": {
          "$ref": "#/definitions/ApiLib.CustomDate"
        }
      },
      "required": [
        "propString",
        "propStringArray1",
        "ideas",
        "extIdeas",
        "feedback",
        "propNumber",
        "propInteger",
        "propBoolean",
        "propDate",
        "userPicture",
        "NilValue",
        "CatOrDog",
        "CatAndDog",
        "PossibleMeetingDate"
      ]
    },
    "ApiLib.Cat": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "color": {
          "type": "string"
        }
      },
      "required": [
        "name",
        "color"
      ]
    },
    "ApiLib.Dog": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "fangs": {
          "type": "string"
        }
      },
      "required": [
        "name",
        "fangs"
      ]
    },
    "ApiLib.CustomDate": {
      "type": "string",
      "x-raml-facets": [
        {
          "onlyFutureDates": "boolean"
        },
        {
          "noHolidays": "boolean"
        }
      ],
      "format": "date"
    },
    "SongsLib.Song": {
      "properties": {
        "title": {
          "type": "string"
        },
        "length": {
          "type": "number"
        }
      },
      "type": "object",
      "required": [
        "title",
        "length"
      ],
      "example": {
        "title": "My Song",
        "length": 12,
        "strict": false,
        "x-raml-example-name": "song1"
      },
      "x-raml-additional-examples": [
        {
          "value": "{\n  \"title\": \"Last\",\n  \"length\": 3\n}",
          "strict": true,
          "structuredValue": {
            "title": "Last",
            "length": 3
          },
          "x-raml-example-name": "song2"
        }
      ]
    }
  }
}